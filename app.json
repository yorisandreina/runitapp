    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yorisandreina.app"
    },
    "web": {
      "favicon": "./assets/logo.png"
    }
  }
}
